            "dependsOn": [
                {
                    "category": "api",
                    "resourceName": "plooralistGraphQL",
                    "attributes": [
                        "GraphQLAPIIdOutput",
                        "GraphQLAPIEndpointOutput"
                    ]
                }
            ]
        },
        "plooralistCreateCouplPanelsFromUsersAreContacts": {
