                    "category": "api",
                    "resourceName": "plooralistGraphQL",
                    "attributes": [
                        "GraphQLAPIIdOutput",
                        "GraphQLAPIEndpointOutput"
                    ]
                },
                {
                    "category": "analytics",
                    "resourceName": "plooralistPinpoint",
                    "attributes": [
                        "Id",
                        "Region"
                    ]
                }
            ]
        }
    },
    "auth": {
        "plooralistAuth": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "plooralistAuthPostConfirmation",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ]
        }
    },
    "api": {
        "plooralistGraphQL": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "authConfig": {
                    "additionalAuthenticationProviders": [
                        {
                            "authenticationType": "AWS_IAM"
                        }
                    ],
                    "defaultAuthentication": {
                        "authenticationType": "AMAZON_COGNITO_USER_POOLS",
                        "userPoolConfig": {
                            "userPoolId": "authplooralistAuth"
                        }
                    }
                }
            }
        }
    },
    "analytics": {
        "plooralistPinpoint": {
            "service": "Pinpoint",
            "providerPlugin": "awscloudformation"
        }
    },
    "notifications": {
        "plooralistPinpoint": {
            "service": "Pinpoint",
            "channels": [
                "APNS",
                "FCM"
            ]
        }
    },
    "storage": {
        "plooralistS3": {
            "service": "S3",
            "providerPlugin": "awscloudformation"
        }
    }
}