            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "api",
                    "resourceName": "plooralistGraphQL",
                    "attributes": [
                        "GraphQLAPIIdOutput",
                        "GraphQLAPIEndpointOutput"
                    ]
                }
            ]
        },
        "plooralistTaskStream": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
